{
    "schemaVersion": 1,
    "id": "civchesttracker",
    "version": "${mod_version}",
    "name": "${mod_name}",
    "description": "${mod_description}",
    "authors": [
        "Protonull"
    ],
    "contact": {
        "homepage": "${mod_home_url}",
        "sources": "${mod_source_url}",
        "issues": "${mod_issues_url}"
    },
    "license": "${copyright_licence}",
    "icon": "assets/civchesttracker/icon.png",
    "environment": "client",
    "entrypoints": {
        "modmenu": [
            "red.jackf.chesttracker.impl.compat.mods.ChestTrackerModMenu"
        ]
    },
    "depends": {
        "java": ">=21",
        "minecraft": "${minecraft_version}",
        "fabricloader": ">=${fabric_loader_version}",
        "fabric": ">=${fabric_api_version}"
    },
    "mixins": [
        "civchesttracker.mixins.json"
    ]
}
